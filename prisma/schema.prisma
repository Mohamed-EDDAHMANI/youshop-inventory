generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Inventory {
  id        String   @id @default(uuid())
  sku       String  @unique
  quantity  Int
  reserved  Int     @default(0)
}
